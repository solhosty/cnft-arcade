{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"module": "es2020",
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": false,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": false,
		"moduleResolution": "node",
		"verbatimModuleSyntax": true,
		"ignoreDeprecations": "5.0",
	}
}